{
  "rules": {
    "indent": [ 1, 2 ],
    "quotes": [ 2, "single"],
    "no-mixed-requires": [ 1, true ],
    "linebreak-style": [ 2, "unix" ],
    "semi": [ 2, "never" ],
    "no-console": 0
  },
  "globals":{
    "process": true,
    "describe": true,
    "it": true,
    "after": true,
    "before": true
  },
  "env": {
    "es6": true,
    "commonjs": true,
    "node": true
  },
  "extends": ["eslint:recommended"]
}
